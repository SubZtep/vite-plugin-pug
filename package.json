{
  "name": "vite-plugin-pug",
  "version": "0.3.1",
  "description": "Vite plugin for transform Pug templates into HTML",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "node scripts/build.js --watch",
    "build": "rm -rf dist && node scripts/build.js && tsc",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SubZtep/vite-plugin-pug.git"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "pug",
    "jade"
  ],
  "author": "Andras Serfozo <subztep@gmail.com>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/SubZtep/vite-plugin-pug/issues"
  },
  "homepage": "https://github.com/SubZtep/vite-plugin-pug#readme",
  "dependencies": {
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@types/pug": "^2.0.6",
    "esbuild": "^0.14.48",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vitest": "^0.16.0"
  }
}
